{
  "name": "nuxtbeats",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A starter template for building cross-platform desktop applications with Nuxt and Tauri.",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare",
    "clean": "nuxi cleanup && nuxi prepare",
    "typecheck": "nuxi typecheck",
    "changelog": "changelogen --release",
    "ci": "bun run lint && bun run typecheck && bun run test",
    "test": "bun --bun x vitest --run",
    "test:snapshots": "bun --bun x vitest --run --update",
    "tauri": "tauri",
    "dev:tauri": "tauri dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix ."
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.38",
    "@iconify-json/simple-icons": "^1.2.33",
    "@nuxt/image": "1.10.0",
    "@nuxt/scripts": "0.11.6",
    "@nuxt/test-utils": "3.17.2",
    "@nuxt/ui": "^3.0.2",
    "@nuxtjs/i18n": "9.5.3",
    "@pinia/nuxt": "0.11.0",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-opener": "^2.2.6",
    "@unhead/vue": "^2.0.3",
    "@vueuse/components": "^13.1.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/integrations": "^13.1.0",
    "@vueuse/motion": "^3.0.3",
    "@vueuse/nuxt": "^13.1.0",
    "@vueuse/router": "^13.1.0",
    "compodium": "0.1.0-beta.5",
    "nuxt": "^3.16.2",
    "nuxt-zod-i18n": "^1.11.5",
    "pinia": "^3.0.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@nuxt/eslint": "^1.4.1",
    "@tauri-apps/cli": "^2.5.0",
    "@vitest/ui": "3.1.2",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.1",
    "eslint": "^9.27.0",
    "happy-dom": "^17.4.4",
    "playwright-core": "^1.52.0",
    "typescript": "5.8.3",
    "vitest": "^3.1.2",
    "vue-tsc": "~2.2.10"
  }
}
